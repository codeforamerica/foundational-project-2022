<h1>How can we contact you?</h1>

<%= form_with model: @benefits_app do |form| %>
  <%= form.label "What's your address?" %><br>
  <%= form.text_field :address %><br>
  <%= form.label "What's your phone number?" %><br>
  <%= form.text_field :phone_number %><br>
  <%= form.label "What's your email address?" %><br>
  <%= form.text_field :email_address %>
  <div>
    <%= form.submit "Continue"%>
  </div>
<% end %>

